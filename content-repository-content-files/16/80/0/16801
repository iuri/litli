		: <!-- eval if false -->
		[[SET:VAR:active=0]]
	]]
	[[EVAL:IF:[[GETDATA:organid:LENGTH]]>0 && [[GETDATA:SearchButton:LENGTH]]>0
		: <!-- eval if true -->
		[[DBASE:SELECT:[Member Table]:(Inactive=[[GETDATA::VAR=active]])
			and (NickName like '%%ORG%%') and (Identifier like '[[GETDATA:organid]]%')
		: <!-- Begin dbase select interlace field -->
<!--webbot bot="HTMLMarkup" endspan -->
