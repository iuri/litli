<!-- Nickname like '%%ORG%%' and LastName like '%[[GETDATA:organ]]%':<br>{LastName}&nbsp&nbsp&nbsp&nbsp{Identifier} -->
<table>
	[[EVAL:IF:[[GETDATA:MemOnce:Length]] >0
	:
		<tr>
		<th colspan="3">
				Clicking View (after a Search) will show detail in the upper left grey box</br>
				For more information or to edit the information press "<b>Show</b>"</br>
				<font size="-2">Inactive members will not be shown in the search</br>
				</font>
		</th>
		</tr>
	:
	]]
	[[EVAL:IF:[[GETDATA:InactiveCheck]]=on
		:
		[[SET:VAR:active=-1]]
		:
		[[SET:VAR:active=0]]
	]] <!-- end eval if -->
	<!-- double check, if first name, then if last name,  else ID -->
	[[EVAL:IF:[[EVAL:BOOL:
		[[EVAL:BOOL:[[GETDATA:SearchButton:LENGTH]]>0]] &&
		[[EVAL:BOOL:
			[[EVAL:BOOL:[[GETDATA:FirstName:LENGTH]]>0]] ||
			[[EVAL:BOOL:[[GETDATA:LastName:LENGTH]]>0]]
		]]
					]]
		:
			[[DBASE:SELECT:[Member Table]:(Inactive=[[GETDATA::VAR=active]])
		  and ((NickName <> '%ORG%') or (NickName IS NULL)) and
			[[EVAL:IF:[[GETDATA:FirstName:LENGTH]]>0
				:
					(FirstName like '[[GETDATA:FirstName]]%')
				:
					(FirstName like '%')
			]] and
			[[EVAL:IF:[[GETDATA:LastName:LENGTH]]>0
				:
					(LastName like '[[GETDATA:LastName]]%')
				:
					(LastName like '%')
			]]
			:<!-- Interlace for DBASE:SELECT Begins here -->
			<tr>
				<td>
					{LastName}
				</td>
				<td>
					{FirstName}
				</td>
				<td>
					{Identifier}
				</td>
				<td>
					<a href=/cgi-bin/virtuflex.exe?template=/td/members.htm&organid=[[GETDATA:organid]]&MemID={MemID}&SiteID={Site}&Sender={MemID}&ProvFindButton=[[GETDATA:ProvFindButton]]&RecFindButton=[[GETDATA:RecFindButton]]>View</a>
				</td>
			</tr>
		]]      <!-- end dbase select -->
: <!-- Eval if false condition here -->
	[[EVAL:IF:[[GETDATA:SearchButton:LENGTH]]>0
		:
			[[DBASE:SELECT:[Member Table]:(Inactive=[[GETDATA::VAR=active]])
				and
				[[EVAL:IF:[[GETDATA:Identifier:LENGTH]]>0
					:
						(Identifier like '%[[GETDATA:Identifier]]%')
					:
						(Identifier like '%')
				]]
				and ((NickName <> '%ORG%') or (NickName IS NULL))
				and (FirstName <> 'null' and LastName <> 'null')
			:
				<tr>
					<td>
						{LastName}
					</td>
					<td>
						{FirstName}
					</td>
					<td>
					<td>
						{Identifier}
						<a href=/cgi-bin/virtuflex.exe?template=/td/members.htm&Label=Member&organid=[[GETDATA:organid]]&MemID={MemID}&SiteID={Site}&Sender={MemID}&ProvFindButton=[[GETDATA:ProvFindButton]]&RecFindButton=[[GETDATA:RecFindButton]]>View</a>
					</td>
			</tr>
		]] <!-- end dbase select -->
		: <!-- no search button -->
	]] <!-- end just identifier -->
]] <!-- end firstname, lastname test -->
		</center> <!-- starts after the 3 search boxes -->
