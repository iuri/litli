<!--webbot bot="HTMLMarkup" startspan -->
	:
	]]
	[[EVAL:IF:[[GETDATA:SearchButton:LENGTH]]>0
		: <!-- Search button pressed -->
			[[EVAL:IF:[[GETDATA:Name:LENGTH]]>0
				: <!-- Search by Site name -->
					[[DBASE:SELECT:[Site Table],[Member Table]:[Site Table].Organization=[Member Table].MemID
						and [Site Table].Inactive=[[GETDATA:Inactive:CONVERT=on=1|CONVERT=off=0]]
						and [Site Table].Name like '%[[GETDATA:Name]]%'
					: <!--Begin DBASE SELECT Interlace-->
<!--webbot bot="HTMLMarkup" endspan -->
					<tr>
						<td>
							{[Site Table].Name}
						</td>
						<td>
							{[Member Table].LastName}
						</td>
						<td>
							{[Member Table].Identifier}
						</td>
						<td>
							<a href=/cgi-bin/virtuflex.exe?template=/td/sites.htm&Label=Site&Name=[[GETDATA:Name]]&Identifier=[[GETDATA:Identifer]]&Organization=[[GETDATA:Organization]]&MemOrgID={MemID}&Manager={Manager}&SiteID={SiteID}>View</a>
						</td>
					</tr>
<!--webbot bot="HTMLMarkup" startspan -->
				]] <!-- End interlace -->
 : <!-- No Site name -->
 [[EVAL:IF:[[GETDATA:Organization:LENGTH]]>0
			: <!-- Organization name search -->
					<!-- FOR TESTING enter DC for Site Name-->
				[[DBASE:SELECT:[Site Table],[Member Table]:
					[Site Table].Organization=[Member Table].MemID
					and [Site Table].Inactive=[[GETDATA:Inactive:CONVERT=on=1|CONVERT=off=0]]
					and [Member Table].LastName like '%[[GETDATA:Organization]]%'
						: <!-- DBASE select interlace begin -->
					<tr>
						<td>
									{[Site Table].Name}
						</td>
						<td>
									{[Member Table].LastName}
						</td>
						<td>
									{[Member Table].Identifier}
						</td>
						<td>
									<a href=/cgi-bin/virtuflex.exe?template=/td/sites.htm&Label=Site&Organization=[[GETDATA:Organization]]&MemOrgID={MemID}&Manager={Manager}&SiteID={SiteID}>View</a>
						</td>
					</tr>
					]] <!-- end DBASE -->
			: <!-- if not Organization -->
				<!-- "Site ID" is actually the organization's Identifier -->
						<!-- FOR TESTING enter time for Organization Name-->
				[[DBASE:SELECT:[Site Table],[Member Table]:
					[Site Table].Inactive=[[GETDATA:Inactive:CONVERT=on=1|CONVERT=off=0]]
					and [Site Table].Organization=[Member Table].MemID
					and
						[[EVAL:IF:[[GETDATA:Identifier:LENGTH]]>0
							:
								[Site Table].Identifier like '%[[GETDATA:SiteID]]%'
							:
								[Site Table].Identifier like '%'
						]]
					: <!-- begin dbase select interlace -->
						<tr>
							<td>
								{[Site Table].Name}
							</td>
							<td>
								{[Member Table].LastName}
							</td>
							<td>
								{[Member Table].Identifier}
							</td>
							<td>
								<a href=/cgi-bin/virtuflex.exe?template=/td/sites.htm*Label=Site&MemOrgID={MemID}&Manager={Manager}&SiteID={SiteID}>View</a>
							</td>
						</tr>
					]] <!-- End DBASE select interlace -->
				]] <!-- end test by Organization -->
			]]<!-- end test by Name -->
]] <!-- End test of Search-->
