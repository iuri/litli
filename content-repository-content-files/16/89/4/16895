<!-- check for either New or Updating -->
[[EVAL:IF:[[EVAL:BOOL:
	[[EVAL:BOOL:[[GETDATA:Once]]=Record
	]] ||
	[[EVAL:BOOL:[[GETDATA:NewButton]]=New
	]]
				  ]]
		:<!-- New or Once  -->
			[[EVAL:IF:[[GETDATA:NewButton]]=New
				:<!-- if new, "starters" -->
<!--webbot bot="HTMLMarkup" endspan -->			 
					<center>
					<font size="+2">
						<b>Enter Information about the New Member</b>
					</font>
					</center>
					<!-- remove any partial records -->
					<!-- must explicitly specify Site because that field is						keyed and ODBC won't delete otherwise -->
<!--webbot bot="HTMLMarkup" startspan -->					       
					[[DBASE:DEL:[Member Table]:Site=1 and Updated is null]]
					<!-- add the new record -->
					[[SET:VAR:Identifier=[[DBASE:SELECT:[System Information]:
						1=1:{MAX(SystemCode)}{MAX(NextToAssign)}]]]]
					[[DBASE:ADD:[Member Table]:
						{Site|1}
						{Balance|0}
						{Identifier|[[GETDATA::VAR=Identifier]]}
						{Availability|0}
						{Entered|[[GETDATA::DATE=m/d/y]] [[GETDATA::TIME]]}
						{Comments|Replace this with comments about the Member.}
					]]
					[[SET:VAR:Identifier=[[EVAL:MATH:[[GETDATA::VAR=Identifier|SUBSTR=5=8]]+1]]]]
					[[DBASE:SQL:Update [System Information] SET
						NextToAssign=[[GETDATA::VAR=Identifier]]
					]]
					<!-- this was returning prior record unless I close and reopen -->
					[[SET:DefaultDbase:c\:\timed\tddemo.mdb]]
					[[SET:VAR:MemID=[[DBASE:SELECT:[Member Table]:
						1=1:{MAX(MemID)}]]]]
					<!-- this is an empty site ID -->
					[[SET:VAR:SiteID=1]]
				: <!-- not New then must be Once, do update here -->
					<!-- (1.)  If 'Once' is Record, then if (2.) all data is present, then update-->
					<!-- AssignID will have been passed to the page -->
					[[SET:VAR:MemID=[[GETDATA:MemID]]]]
					[[SET:VAR:SiteID=[[GETDATA:SiteID]]]]
					[[EVAL:IF:[[EVAL:BOOL:
						[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:FirstName:LENGTH]] != 0]] &&
							[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:LastName:LENGTH]] != 0]] &&
								[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:Street:LENGTH]] != 0]] &&
									[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:HomePhone:LENGTH]] != 0]] &&
										[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:WorkPhone:LENGTH]] != 0]] &&
											[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:City:LENGTH]] != 0]] &&
												[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:State:LENGTH]] != 0]] &&
													[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:Zipcode:LENGTH]] != 0]] &&
														[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:Identifier:LENGTH]] != 0]] &&
															[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:Balance:LENGTH]] != 0]] &&
																[[EVAL:BOOL:[[EVAL:BOOL:[[GETDATA:CoordAlfa:LENGTH]] != 0]] &&
																	[[EVAL:BOOL:[[GETDATA:CoordNum:LENGTH]] != 0]]
																]]
															]]
														]]
													]]
												]]
											]]
										]]
									]]
								]]
							]]
						]]
								]]
					: <!-- TRUE -- all data good -->
						<!-- if this was in BASIC it would look like --
						DIM VAR(21)
						X=0
						VAR(0)=1
						VAR(20)=1
						FOR I=20 to 1 STEP -1
						X=X+(2*VAR(I))^I
						NEXT
						X=X+VAR(0)
						PRINT "Availability=";X
						where X=1048575 when these 2 cells == 1,
						Sunday morning, and Saturday evening -->
						[[SET:VAR:I=20]]
						[[SET:VAR:AVAILABILITY=0]]
						[[EVAL:WHILE:[[GETDATA::VAR=I]] >0
							: <!-- body of within loop -->
								[[SET:VAR:Availability=[[EVAL:MATH:
									[[GETDATA::VAR=Availability]] +
									[[EVAL:MATH:
										[[EVAL:MATH:
											2*[[GETDATA:[[GETDATA::VAR=I]]Check:CONVERT=on=1|CONVERT=off=0]]
										]]
										^[[GETDATA::VAR=I]]
									]]
								]]]]
								[[SET:VAR:I=[[GETDATA::VAR=I|DECR]]]]
						]]<!-- end of update loop (EVAL WHILE) -->
						<!-- when VirtuFlex numbers exceed a million, they have
						 a decimal and 6 trailing zeros, remove .000000 -->
						[[SET:VAR:Availability=[[GETDATA::VAR=Availability|PREPEND=`|PARSE=`=.]]]]
						[[SET:VAR:MemSince=[[GETDATA:MemSinceMMSelect]]/[[GETDATA:MemSinceDDSelect]]/[[GETDATA:MemSinceYYSelect]] 0\:0\:0]]
						[[DBASE:SQL:Update [Member Table] SET
							FirstName='[[GETDATA:FirstName]]',
							LastName='[[GETDATA:LastName]]',
							HomePhone='[[GETDATA:HomePhone]]',
							WorkPhone='[[GETDATA:WorkPhone]]',
							Street='[[GETDATA:Street]]',
							City='[[GETDATA:City]]',
							State='[[GETDATA:State]]',
							Zipcode='[[GETDATA:Zipcode]]',
							Identifier='[[GETDATA:Identifier]]',
							Balance=[[GETDATA:Balance]],
							CoordAlfa='[[GETDATA:CoordAlfa]]',
							CoordNum='[[GETDATA:CoordNum]]',
							Availability=[[GETDATA::VAR=Availability]],
							UserNum=
								[[EVAL:IF:[[GETDATA:UserNum:LENGTH]] != 0
					:'[[GETDATA:UserNum:CONVERT='='']]'
					:' '
								]],
							SignedForms=
							[[EVAL:IF:[[GETDATA:SignedForms]]=on
								:
								   -1
							  :
								   0
							]],
							Inactive=
							[[EVAL:IF:[[GETDATA:Inactive]]=on
								:
								  -1
								:
								   0
							]],
							MemSince=[[GETDATA::VAR=MemSince|PREPEND='|CONVERT= = |POSTPEND='|CONVERT=' 1/ 1/1900 0\:0\:0'=null]],
							Comments=
							[[EVAL:IF:[[GETDATA:Comments:LENGTH]] != 0
				:
									'[[GETDATA:Comments:CONVERT='='']]'
					:
									' '
							]],
							Emergency=
							[[EVAL:IF:[[GETDATA:Emergency:LENGTH]] != 0
				:
									'[[GETDATA:Emergency:CONVERT='='']]'
					:
									' '
							]],
							Doctor=
							[[EVAL:IF:[[GETDATA:Doctor:LENGTH]] != 0
					:
									'[[GETDATA:Doctor:CONVERT='='']]'
					:
									' '
							]],
							Updated='[[GETDATA::DATE=m/d/y]] [[GETDATA::TIME]]'
							where MemID=[[GETDATA::VAR=MemID]]
						]] <!-- DBASE update -->
					  <!-- double quote ok in SQL, but single quote(apostrophe) needs to be doubled-->
					<!-- update database each time user submits current page (with data)-->
					:
						<!-- missing data-->
					[[GETDATA:XYZZY:CHECK=/td/OneEer.htm]]
				]] <!-- end data check -->
<!--webbot bot="HTMLMarkup" endspan -->	
				<font size="+2">
				<center>
				<b>
                Updated Volunteer Member
				<img src="/td/blank.gif" alt width="50" height="1">
				</b>
				</center>
<!--webbot bot="HTMLMarkup" startspan -->	
			]] <!-- end not New, was Once -->
		: <!-- not New or Once -->
	<!-- no code at this indent level -->
<!--webbot bot="HTMLMarkup" endspan -->	
	<font size="+2">
	<center>
	<b>
	Volunteer Member
	</font>
	</b>
	</center>
<!--webbot bot="HTMLMarkup" startspan -->	
		[[SET:VAR:MemID=[[GETDATA:MemID]]]]
		[[SET:VAR:SiteID=[[GETDATA:SiteID]]]]
]]
<!-- Eli made this qualifier 'Sender'
      tried to send from members.htm, etc. -->
[[SET:VAR:MemSinceIsNotNull=[[DBASE:SELECT:[Member Table]
	:MemID=[[GETDATA::VAR=MemID]]
	AND MemSince is not null AND
	MemSince <>  1/ 1/1900
	:
	{Count(MemID)}]]]]
[[SET:VAR:Availability=[[DBASE:SELECT:[Member Table]
	:MemID=[[GETDATA::VAR=MemID]]
	:
	{Availability}]]]]
<!-- true VirtuFlex bug -- all math results in >= 0, never negative -->
<!-- next operation if written in Basic (X being Availability)
	FOR I=20 TO 0 STEP -1
	IF 2^I-X > 0 THEN ELSE PRINT I (colon) X=X-2^I
	NEXT
	-->
[[SET:VAR:I=20]]
[[EVAL:WHILE:[[GETDATA::VAR=I]]>=0
	: <!-- body of within loop -->
		[[EVAL:IF:[[EVAL:MATH:
				[[EVAL:MATH:
					2^[[GETDATA::VAR=I]]-[[GETDATA::VAR=Availability]]
																			]]
							]] > 0
			:<!-- true -->
				[[SET:VAR:[[GETDATA::VAR=I]]Check=]]
			: <!-- false -->
				[[SET:VAR:[[GETDATA::VAR=I]]Check=CHECKED]]
				[[SET:VAR:Availability=[[EVAL:MATH:
					[[GETDATA::VAR=Availability]]-2^[[GETDATA::VAR=I]]
															 ]]]]
		]] <!-- end EVAL IF -->
		[[SET:VAR:I=[[GETDATA::VAR=I|DECR]]]]
]]<!-- end of update loop (EVAL WHILE) -->
